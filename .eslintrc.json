{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "no-undef": [
      "error",
      {
        "typeof": false
      }
    ],
    "no-empty-function": [
      "error",
      {
        "allow": null
      }
    ],
    "no-duplicate-imports": [
      "error",
      {
        "includeExports": false
      }
    ]
  }
}